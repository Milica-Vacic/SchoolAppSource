<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Student_Passing_Credit_Threshold_NotNull</fullName>
    <active>true</active>
    <errorConditionFormula>AND( RecordType.Name  = &apos;Student&apos;, ISNULL(Passing_Credit_Threshold__c))</errorConditionFormula>
    <errorDisplayField>Passing_Credit_Threshold__c</errorDisplayField>
    <errorMessage>Student must have value for Passing Credit Threshold</errorMessage>
</ValidationRule>
