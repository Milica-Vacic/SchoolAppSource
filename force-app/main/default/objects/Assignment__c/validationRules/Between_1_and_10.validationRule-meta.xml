<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Between_1_and_10</fullName>
    <active>true</active>
    <errorConditionFormula>Max_Score__c &lt;1 ||  Max_Score__c &gt;10</errorConditionFormula>
    <errorDisplayField>Max_Score__c</errorDisplayField>
    <errorMessage>Max score must be between 1 and 10</errorMessage>
</ValidationRule>
