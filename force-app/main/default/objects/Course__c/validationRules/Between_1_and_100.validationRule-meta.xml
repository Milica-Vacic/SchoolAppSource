<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Between_1_and_100</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Credit__c &lt;1,Credit__c &gt;100)</errorConditionFormula>
    <errorDisplayField>Credit__c</errorDisplayField>
    <errorMessage>Credit must be between 1 and 100</errorMessage>
</ValidationRule>
