<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_be_unchecked</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISNEW())  &amp;&amp;   NOT(Completed__c)&amp;&amp;  ISCHANGED(Completed__c)</errorConditionFormula>
    <errorDisplayField>Completed__c</errorDisplayField>
    <errorMessage>The course cannot be uncompleted</errorMessage>
</ValidationRule>
